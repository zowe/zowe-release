{ 
    "zowe" : {
        "from" : "libs-snapshot-local",
        "to" : "libs-release-local",
        "sourcePath" : "org/zowe/{ZOWE_ARTIFACT_DIR}",
        "sourceFiles": {
            "zowe-*.pax": "zowe-*.pax",
            "zowe-smpe-*.zip": "zowe-smpe-*.zip",
            "smpe-promote-*.tar": "smpe-promote-*.tar",
            "server-bundle.amd64-*.tar": "server-bundle.amd64-*.tar",
            "server-bundle.sources.amd64-*.tar": "server-bundle.sources.amd64-*.tar",
            "server-bundle.s390x-*.tar": "server-bundle.s390x-*.tar",
            "server-bundle.sources.s390x-*.tar": "server-bundle.sources.s390x-*.tar",
            "zowe-containerization-*.zip": "zowe-containerization-*.zip"
        }
    },

    "zowe-cli" : {
        "nightlyFrom" : "libs-snapshot-local",
        "from" : "libs-release-local",
        "to" : "libs-release-local",
        "sourcePath" : "org/zowe/cli",
        "sourceFiles": {
            "zowe-cli-package-*.zip": "zowe-cli-package-1*.zip",
            "zowe-cli-plugins-*.zip": "zowe-cli-plugins-1*.zip"
        }
    },

    "zowe-cli-sdk" : {
        "from" : "libs-release-local",
        "to" : "libs-release-local",
        "sourcePath" : "org/zowe/sdk",
        "sourceFiles": {
            "zowe-python-sdk-*.zip": "zowe-python-sdk-1*.zip",
            "zowe-nodejs-sdk-*.zip": "zowe-nodejs-sdk-1*.zip",
            "zowe-nodejs-sdk-typedoc-*.zip": "zowe-nodejs-sdk-typedoc-1*.zip"
        }
    },

    "zowe-cli-next" : {
        "from" : "libs-release-local",
        "sourcePath" : "org/zowe/cli",
        "sourceFiles": {
            "zowe-cli-package-next-*.zip": "zowe-cli-package-next-*.zip",
            "zowe-cli-plugins-next-*.zip": "zowe-cli-plugins-next-*.zip"
        },
        "to" : "libs-release-local",
        "targetPath": "org/zowe/next"
    },

    "zowe-cli-sdk-next" : {
        "from" : "libs-release-local",
        "sourcePath" : "org/zowe/sdk",
        "sourceFiles": {
            "zowe-nodejs-sdk-next-*.zip": "zowe-nodejs-sdk-next-*.zip"
        },
        "to" : "libs-release-local",
        "targetPath": "org/zowe/next"
    }
}